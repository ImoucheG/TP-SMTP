;
; BIND data file for local loopback interface
;
$TTL	604800
@		IN	SOA		ns.gira.labos-nantes.ovh. root.gira.labos-nantes.ovh. (
			      1		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL

@		IN	NS		ns.
@		IN	MX 10 	mail.gira.labos-nantes.ovh.

ns		IN	A		172.16.4.1
mail	IN	A		172.16.4.1
pop		IN	CNAME	mail