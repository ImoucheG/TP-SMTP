;
; BIND reverse data file for local loopback interface
;
$TTL	604800
@	IN	SOA	hostone.labos-nantes.ovh. root.labos-nantes.ovh. (
			      1		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	hostone.
101	IN	PTR	hostone.labos-nantes.ovh.
